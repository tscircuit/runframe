{
  "name": "@tscircuit/runframe",
  "main": "dist/preview.js",
  "type": "module",
  "version": "0.0.44",
  "exports": {
    ".": "./dist/preview.js",
    "./preview": "./dist/preview.js",
    "./runner": "./dist/runner.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cosmos",
    "build": "bun run build:css && bun run build:standalone && bun run build:lib",
    "build:lib": "tsup-node lib/preview.ts lib/runner.ts --platform browser --format esm --dts",
    "build:css": "bun run scripts/build-css.ts",
    "build:site": "cosmos-export",
    "build:standalone": "STANDALONE=1 vite build",
    "analyze": "source-map-explorer 'dist/standalone.min.js'",
    "format:check": "biome format .",
    "format": "biome format --write .",
    "vercel-build": "bun run build:css && cosmos-export"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/debug": "^4.1.12",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "circuit-json": "^0.0.122",
    "class-variance-authority": "^0.7.1",
    "debug": "^4.4.0",
    "playwright": "^1.49.1",
    "postcss": "^8.4.49",
    "react": "18",
    "react-cosmos": "^6.2.1",
    "react-dom": "18",
    "rollup-plugin-visualizer": "^5.12.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.5",
    "vite": "^6.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tscircuit/3d-viewer": "^0.0.77",
    "@tscircuit/core": "^0.0.247",
    "@tscircuit/eval-webworker": "^0.0.44",
    "@tscircuit/file-server": "^0.0.12",
    "@tscircuit/assembly-viewer": "^0.0.1",
    "@tscircuit/pcb-viewer": "^1.10.22",
    "@tscircuit/props": "^0.0.123",
    "@tscircuit/schematic-viewer": "2.0.9",
    "circuit-to-svg": "^0.0.99",
    "comlink": "^4.4.2",
    "cssnano": "^7.0.6",
    "lucide-react": "^0.468.0",
    "react-cosmos-plugin-vite": "^6.2.0",
    "schematic-symbols": "^0.0.111",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  }
}
